    "app-root-path": "^3.0.0",
    "rimraf": "3.0.2",
    "tar-fs": "^2.1.1"
  },
  "devDependencies": {
    "clean-jsdoc-theme": "3.0.7",
    "eslint": "7.4.0",
    "eslint-plugin-jest": "23.18.0",
    "gh-pages": "2.2.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "jsdoc": "3.6.6",
    "prettier": "2.0.5"
  }
