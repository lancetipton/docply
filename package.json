{
  "name": "docply",
  "version": "0.0.0",
  "description": "Manipulate internal config of docker images",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lancetipton/docply.git"
  },
  "keywords": [
    "docker",
    "image",
    "container",
    "labels"
  ],
  "author": "Lance Tipton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lancetipton/docply/issues"
  },
  "homepage": "https://github.com/lancetipton/docply",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "engines": {
